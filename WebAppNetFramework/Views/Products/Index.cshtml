@model IEnumerable<WebAppNetFramework.Models.Product>

@{
    ViewBag.Title = "Products";
}

<h2>Products from:</h2>
<h4>@ViewBag.Message</h4>

<table class="table">
    <tr>
        <th>Name</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
        </tr>
    }

</table>


<script>
    console.log('@ViewBag.Message');
</script>